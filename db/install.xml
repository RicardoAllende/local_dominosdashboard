<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/dominosdashboard/db" VERSION="20141018" COMMENT="XMLDB file for Moodle local/dominosdashboard"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="dominos_kpis" COMMENT="">
            <FIELDS>
                <FIELD NAME="id"            TYPE="int"   LENGTH="10"  NOTNULL="true"   SEQUENCE="true" />
                <FIELD NAME="kpi"           TYPE="char"   LENGTH="10" NOTNULL="false"    SEQUENCE="false" />
                <FIELD NAME="nombre"        TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="valor"         TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="typevalue"     TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="day"           TYPE="char"  LENGTH="50"  NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="week"          TYPE="char"  LENGTH="50"  NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="month"         TYPE="char"  LENGTH="50"  NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="year"          TYPE="char"  LENGTH="50"  NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="region"        TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="ccosto"        TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="nom_ccosto"    TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="original_time" TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="distrital"     TYPE="char"  LENGTH="255" NOTNULL="false"  SEQUENCE="false" />
                <FIELD NAME="timecreated"   TYPE="int"   LENGTH="10"  NOTNULL="false"  SEQUENCE="false" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
        </TABLE>
        <TABLE NAME="dominos_historico" COMMENT="">
            <FIELDS>
                <FIELD NAME="id"                TYPE="int"  LENGTH="10"     NOTNULL="true"  SEQUENCE="true" />
                <FIELD NAME="courseid"          TYPE="int"  LENGTH="10"     NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="shortname"         TYPE="char" LENGTH="255"    NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="fullname"          TYPE="char" LENGTH="255"    NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="enrolled_users"    TYPE="int"  LENGTH="9"      NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="approved_users"    TYPE="int"  LENGTH="9"      NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="filterid"          TYPE="char" LENGTH="50"     NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="filtertext"        TYPE="char" LENGTH="255"    NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="timecreated"       TYPE="int"  LENGTH="10"     NOTNULL="false" SEQUENCE="false" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>